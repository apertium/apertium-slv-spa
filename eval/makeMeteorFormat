#1 parameter src ali tst ali ref
#2 parameter source (SL)
#3 parameter target (SR)
#4 parameter full filename (slsrorwell)
echo "<$1set setid=\"meteor-$1\" srclang=\"$2\" trglang=\"$3\">"
echo "<DOC docid=\"$4\" sysid=\"12\">"
echo "<SEG>"
tst="tst"
src="src"
ref="ref"
cat $4 |sed ':a;N;$!ba;s@\n@</sed><\sed>\n @g'
echo "</SEG>" 
echo "</DOC>"
echo "</$1set>"
